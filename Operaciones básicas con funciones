#operaciones
def suma(valor1,valor2):
    total = int(valor1)+int(valor2)

    return total

def resta(valor1,valor2):
    total = int(valor1)-int(valor2)
    return total

def multiplicacion(valor1,valor2):
    total = int(valor1)*int(valor2)

    return total

def division(valor1,valor2):
    total = int(valor1)/int(valor2)

    return total

#Llamar la funcion


valor1 = input()
valor2 = input()

total_suma = suma(valor1, valor2)
total_resta = resta(valor1, valor2)
total_multipliacion = multiplicacion(valor1, valor2)
total_division = division(valor1,valor2)

print(total_suma)
print(total_resta)
print(total_multipliacion)
print(total_division)

